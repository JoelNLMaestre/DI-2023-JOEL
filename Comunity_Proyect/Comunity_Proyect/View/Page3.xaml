<Page x:Class="Comunity_Proyect.View.Page3"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Comunity_Proyect.View"
      mc:Ignorable="d" 
      d:DesignHeight="600" d:DesignWidth="500"
      Title="Page3">

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="4*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>

        <TextBlock Text="Mantenimimento" FontSize="40" Margin="20" VerticalAlignment="Center" HorizontalAlignment="Center" Grid.Row="0"/>

        <TextBlock x:Name="textBox" Grid.Row="1" FontSize="15"/>

        <Button x:Name="volverbttn" Content="volver" Width="100" Height="40" HorizontalAlignment="Center" VerticalAlignment="Center"  Grid.Row="2"/>

    </Grid>
</Page>
